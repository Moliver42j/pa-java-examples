package examplesfortechtest;

import static org.junit.Assert.*;

import org.junit.Test;

public class BalancedBracketsTest {

	@Test
	public void test1() {
		String test1 = BalancedBracketsAgain.balancedBrackets("[()]");
		
		assertEquals("Balanced",test1);
	}
	
	@Test
	public void test2() {
		String test2 = BalancedBracketsAgain.balancedBrackets("{[]}");
		
		assertEquals("Balanced",test2);
	}
	
	@Test
	public void test3() {
		String test3 = BalancedBracketsAgain.balancedBrackets("[]{}()");
		
		assertEquals("Balanced",test3);
	}
	
	@Test
	public void test4() {
		String test4 = BalancedBracketsAgain.balancedBrackets("[)[)");
		
		assertEquals("Not Balanced",test4);
	}
	
	@Test
	public void test5() {
		String test5 = BalancedBracketsAgain.balancedBrackets("()]()");
		
		assertEquals("Not Balanced",test5);
	}
	
	@Test
	public void test6() {
		String test6 = BalancedBracketsAgain.balancedBrackets("{[}]");
		
		assertEquals("Not Balanced",test6);
	}
	
	@Test
	public void test7() {
		String test7 = BalancedBracketsAgain.balancedBrackets("{({[]}())}");
		
		assertEquals("Balanced",test7);
	}
	@Test
	public void testTooManyParen() {
		String test7 = BalancedBracketsAgain.balancedBrackets("((((((((()");
		
		assertEquals("Not Balanced",test7);
	}
	
	@Test
	public void testTooManySquare() {
		String test7 = BalancedBracketsAgain.balancedBrackets("[]]]]]]]]");
		
		assertEquals("Not Balanced",test7);
	}
	
	@Test
	public void testTooManyCurly() {
		String test7 = BalancedBracketsAgain.balancedBrackets("{{}}}");
		
		assertEquals("Not Balanced",test7);
	}
	
	@Test
	public void testSpaces() {
		String test7 = BalancedBracketsAgain.balancedBrackets("{ { }}");
		
		assertEquals("Balanced",test7);
	}
	
	@Test
	public void testOpposites() {
		String test7 = BalancedBracketsAgain.balancedBrackets(")}{(");
		
		assertEquals("Not Balanced",test7);
	}


}
